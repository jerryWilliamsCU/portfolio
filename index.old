  #portfolio{
    margin: 0 auto;
    margin-bottom: 10em;
    background-color: $color5;
  }
  section#portfolio #gallery [class^='col'] {
    position: relative;
    overflow: hidden;
    
  }
  section#portfolio #gallery [class^='col'] img {
    height: 333px;
    width: 100%;
    border: $color5 solid 1px;
    
  }

  
  
  section#portfolio #gallery [class^='col'] .img-overlay {
    opacity: 0;
    position: absolute;
    top: 100%;
    left: 0;
    transition: top 0.5s, opacity 0.1s;
    transition-timing-function: ease-in-out;
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    background-color: rgba(0, 0, 0, .3);
    backdrop-filter: blur(8px);
    cursor: pointer;
    
    
    h3{
      color: $color4;
      font-size: 1.4em;
    }

    .fab, .fas{
      font-size: 1.3em;

    }
  
  }
  section#portfolio #gallery [class^='col']:hover .img-overlay {
    top: 0;
    opacity: 1;
  }
  section#portfolio #gallery [class^='col'] span, section#portfolio #gallery [class^='col'] h3 {
    color: $color3;
  }
  /* Global Styles */
* {
  box-sizing: border-box;
}

html {
  position: relative;
  scroll-behavior: smooth;

}

body {
  background-color: black;
  font-family: $secondary-font;
  font-size: 28px;
  font-weight: 200;
  line-height: 1;
  margin: 0;
  padding: 0;
}

h1, h2, h3, h4, h5, h6, ul, ol, p {
  margin-top: 0;
  color: $color4;
}

h1, h2, h3 {
  font-weight: 400;
  color: $color4;
  font-family: $primary-font;
  text-align: center;
  
}
.h3{
  font-family: $primary-font;
  color: $color4;

}
p {
  line-height: 1.5;
}

.sec-services{
  height: 100vh;
  display: flex;
  padding-top: 150px;
  
  text-align: center;
  background: $color2;
  
}
#tech{ 
  .h3{
  animation: flicker linear infinite 2.5s;
  animation-fill-mode: both;
  color: $color4;
  line-height: 1em;
  }
  .fab, .fas{
    color: $color3;
    font-size: 1.8em;
    line-height: 1.3em;
    letter-spacing: .2em;
  }


